<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".View.Fragment.SentieroInformazioniFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.Barrier
            android:id="@+id/barrier3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:barrierDirection="bottom"
            app:constraint_referenced_ids="descriptionFragment"
            tools:layout_editor_absoluteX="411dp"
            tools:layout_editor_absoluteY="891dp" />

        <TextView
            android:id="@+id/descriptionFragment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_medium"
            android:paddingStart="15dp"
            android:paddingLeft="25dp"
            android:paddingTop="25dp"
            android:paddingRight="25dp"
            android:paddingBottom="25dp"
            android:maxLines="10"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/nomePercorsoFragment" />

        <ImageView
            android:id="@+id/propicFragment"
            android:layout_width="81dp"
            android:layout_height="76dp"
            android:layout_marginStart="4dp"
            android:layout_marginEnd="16dp"
            android:src="@drawable/icons8_male_user_96"
            app:layout_constraintBottom_toTopOf="@+id/view2"
            app:layout_constraintEnd_toStartOf="@+id/LocalitaFragment"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/LocalitaFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginBottom="32dp"
            android:text="LocalitÃ "
            android:maxLength="16"
            app:layout_constraintBottom_toTopOf="@+id/view2"
            app:layout_constraintStart_toEndOf="@+id/propicFragment"
            app:layout_constraintTop_toBottomOf="@+id/profileNameFragment" />

        <TextView
            android:id="@+id/timeFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginEnd="22dp"
            android:layout_marginBottom="32dp"
            android:layout_weight="1"
            android:drawableLeft="@drawable/ic_time"
            android:drawablePadding="2dp"
            android:paddingStart="8dp"
            android:text="TextView"
            app:layout_constraintBottom_toTopOf="@+id/view2"
            app:layout_constraintEnd_toStartOf="@+id/disableFragment"
            app:layout_constraintStart_toEndOf="@+id/LocalitaFragment"
            app:layout_constraintTop_toBottomOf="@+id/difficultyFragment" />

        <TextView
            android:id="@+id/difficultyFragment"
            android:layout_width="89dp"
            android:layout_height="25dp"
            android:layout_marginTop="24dp"
            android:layout_marginBottom="8dp"
            android:layout_weight="1"
            android:drawableLeft="@drawable/ic_difficile"
            android:drawablePadding="2dp"
            android:paddingStart="8dp"
            android:text="TextView"
            app:layout_constraintBottom_toTopOf="@+id/timeFragment"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.617"
            app:layout_constraintStart_toEndOf="@+id/profileNameFragment"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/disableFragment"
            android:layout_width="29dp"
            android:layout_height="24dp"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="32dp"
            android:layout_weight="1"
            android:drawableRight="@drawable/ic_disabile"
            android:drawablePadding="2dp"
            android:paddingStart="8dp"
            app:layout_constraintBottom_toTopOf="@+id/view2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/timeFragment"
            app:layout_constraintTop_toBottomOf="@+id/difficultyFragment" />

        <TextView
            android:id="@+id/profileNameFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="24dp"
            android:layout_marginBottom="16dp"
            android:maxLength="16"
            android:text="Nome"
            app:layout_constraintBottom_toTopOf="@+id/LocalitaFragment"
            app:layout_constraintStart_toEndOf="@+id/propicFragment"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/view2"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#c0c0c0"
            app:layout_constraintBottom_toTopOf="@+id/nomePercorsoFragment"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/propicFragment" />

        <TextView
            android:id="@+id/nomePercorsoFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:maxLines="1"
            android:text="TextView"
            android:textSize="24sp"
            android:textStyle="bold"

            app:layout_constraintBottom_toTopOf="@+id/descriptionFragment"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view2" />

        <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
            android:id="@+id/buttonOpinione"
            android:layout_width="272dp"
            android:layout_height="56dp"
            android:layout_gravity="bottom|end"
            android:layout_margin="16dp"
            android:layout_marginStart="123dp"
            android:layout_marginEnd="67dp"
            android:contentDescription="Login"
            android:text="Esprimi un opinione"
            android:textColor="#000000"
            android:textColorLink="@color/purple_200"
            app:backgroundTint="#91C1A4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />

        <TextView
            android:id="@+id/txtAlert"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_marginTop="75dp"
            android:layout_marginBottom="1dp"
            android:textColor="#EF6C00"
            app:autoSizeMinTextSize="2sp"
            app:autoSizeTextType="uniform"
            app:layout_constraintBottom_toTopOf="@+id/nomePercorsoFragment"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.467"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/propicFragment"
            app:layout_constraintVertical_bias="1.0" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>